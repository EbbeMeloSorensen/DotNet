<Application x:Class="DD.UI.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DD.UI.WPF"
             xmlns:viewModel="clr-namespace:DD.ViewModel;assembly=DD.ViewModel"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ObjectDataProvider x:Key="BootStrapper" ObjectType="local:BootStrapper"/>
        <DataTemplate DataType="{x:Type viewModel:CreateCreatureTypeDialogViewModel}">
            <local:CreateCreatureTypeDialogView/>
        </DataTemplate>
    </Application.Resources>
</Application>
